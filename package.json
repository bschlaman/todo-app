		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-webpack-plugin": "^5.5.0",
		"live-server": "^1.2.2",
		"npm-run-all": "^4.1.5",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.0"
	}
