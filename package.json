		"url": "https://github.com/bschlaman/todo-app/issues"
	},
	"homepage": "https://github.com/bschlaman/todo-app#readme",
	"dependencies": {
		"dompurify": "^2.4.0",
		"marked": "^4.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-markdown": "^8.0.7"
	},
	"devDependencies": {
		"@types/dompurify": "^2.4.0",
		"@types/marked": "^4.0.7",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.1",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"css-loader": "^6.7.2",
		"eslint": "^8.27.0",
