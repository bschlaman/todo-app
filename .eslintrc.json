{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "standard-with-typescript"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"consistent-return": "error",
		"no-else-return": "error",
		"space-unary-ops": "error"
	}
}
